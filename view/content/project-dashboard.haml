          .user-info__name
            Borja
      .project-card__header-text Changed code associated with note. November 27, 2015
  .project-card__main
    .paths
      .paths__wrapper
        .paths__item.paths__item--branch master
      .paths__wrapper
        .paths__item.paths__item--commit commit name
      .paths__wrapper
        .paths__item.paths__item--file https:// lorem / ipsum / 21 / index.js
