    .card__code
      %code.code-block
        %table.code-block__table{cellspacing:'0',cellpadding:'0',border:'0'}
          %tr.code-block__row
            %td.code-block__cell.code-block__cell--number 1
            %td.code-block__cell.code-block__cell--code const getItem = document.getElementById('id');
          %tr.code-block__row
            %td.code-block__cell.code-block__cell--number 2
